{
  "name": "suanime",
  "productName": "suanime",
  "version": "0.9.1",
  "description": "suanime",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "build-prod": "rimraf build && webpack --config webpack.prod.config.js && cpx app/*.{html,ico} build",
    "start": "electron .",
    "dist": "electron-builder",
    "pack": "electron-builder --dir",
    "publish": "build --win -p always",
    "build-pack": "npm run build-prod && npm run pack"
  },
  "build": {
    "target": [
      "nsis"
    ],
    "nsis": {
      "oneClick": "false",
      "allowToChangeInstallationDirectory": true,
      "runAfterFinish": false,
      "installerIcon": "build/icon.ico",
      "uninstallerIcon": "build/icon.ico"
    },
    "appId": "suanime",
    "productName": "suanime",
    "files": [
      "index.js",
      "build/*",
      "build/**/*"
    ],
    "publish": [
      {
        "provider": "github",
        "owner": "fumetsuu",
        "repo": "suanime"
      }
    ]
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-dynamic-import-webpack": "^1.0.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "bytes": "^3.0.0",
    "classnames": "^2.2.5",
    "css-loader": "^0.28.11",
    "didyoumean2": "^1.3.0",
    "electron": "^1.8.4",
    "electron-builder": "^20.8.1",
    "electron-log": "^2.2.14",
    "electron-react-devtools": "^0.5.3",
    "electron-reload": "^1.2.2",
    "ent": "^2.2.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "html-entities": "^1.2.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "github:webpack-contrib/html-webpack-plugin",
    "html2plaintext": "^2.0.1",
    "image-cache": "^1.0.1",
    "loadable-components": "^1.2.0",
    "mt-downloader": "^2.2.2",
    "muxer": "^1.0.1",
    "node-sass": "^4.8.3",
    "popura": "^1.2.5",
    "progress-stream": "^2.0.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-dropdown": "^1.4.0",
    "react-player": "^1.3.1",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-window-titlebar": "^1.0.3",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "request": "^2.85.0",
    "request-progress": "^3.0.0",
    "request-promise": "^4.2.2",
    "sass-loader": "^6.0.7",
    "screenfull": "^3.3.2",
    "ssl-root-cas": "^1.2.5",
    "style-loader": "^0.19.1",
    "su-titlebar": "^1.2.1",
    "uglifyjs-webpack-plugin": "^1.2.4",
    "webpack": "^4.3.0",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-cli": "^2.0.13",
    "webpack-merge": "^4.1.2"
  },
  "dependencies": {
    "electron-debug": "^1.5.0",
    "electron-store": "^1.3.0",
    "electron-updater": "^2.21.5"
  }
